{
    "name": "marcusirgens/jsend-factory",
    "description": "JSend factories for PSR-7",
    "type": "library",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.16",
        "phpunit/phpunit": "^9.3",
        "nyholm/psr7": "^1.3",
        "guzzlehttp/psr7": "^1.6",
        "slim/psr7": "^1.2",
        "http-interop/http-factory-guzzle": "^1.0",
        "slevomat/coding-standard": "^6.4",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Pettersen Irgens",
            "email": "marcus.pettersen.irgens@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MarcusIrgens\\JSendFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcusIrgens\\JSendFactory\\": [
                "tests/unit"
            ]
        }
    }
}
